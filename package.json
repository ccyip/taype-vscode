{
  "name": "taype",
  "displayName": "Taype",
  "description": "Syntax highlighting for the taype language",
  "version": "0.3.1",
  "repository": "https://github.com/ccyip/taype-vscode",
  "publisher": "ccyip",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "taype",
      "aliases": ["Taype", "taype"],
      "extensions": [".tp", ".tpc", ".oil"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "taype",
      "scopeName": "source.taype",
      "path": "./syntaxes/taype.tmLanguage.json"
    }]
  }
}
